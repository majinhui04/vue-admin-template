        "new": "plop",
        "release": "sh build/release.sh"
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint",
            "git add"
        ]
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "echarts": "4.2.1",
        "element-ui": "2.8.2",
        "js-cookie": "2.2.0",
        "sharegoods-ui": "0.0.30",
        "vue": "2.5.17",
        "vue-router": "3.0.1",
        "vuex": "3.0.1"
    },
    "devDependencies": {
        "@babel/core": "7.0.0",
        "@babel/register": "7.0.0",
        "@vue/cli-plugin-babel": "3.7.0",
        "@vue/cli-plugin-eslint": "3.7.0",
        "@vue/cli-service": "3.7.0",
        "@vue/eslint-config-standard": "4.0.0",
        "babel-eslint": "10.0.1",
        "chalk": "2.4.1",
        "chokidar": "2.1.5",
        "connect": "^3.7.0",
        "eslint": "5.16.0",
        "eslint-plugin-vue": "5.0.0",
