        "node-sass": "^4.9.0",
        "plop": "^2.4.0",
        "runjs": "^4.4.2",
        "sass-loader": "^7.1.0",
        "serve-static": "^1.14.1",
        "svg-sprite-loader": "^4.1.6",
        "svgo": "^1.2.2",
        "terser-webpack-plugin": "^1.3.0",
        "vue-template-compiler": "2.5.17"
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
